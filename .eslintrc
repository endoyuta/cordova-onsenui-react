{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "class-methods-use-this": 0,
        "import/no-unresolved": 0,
        "linebreak-style": 0
    },
    "env": {
        "browser": true,
        "node": true
    }
}